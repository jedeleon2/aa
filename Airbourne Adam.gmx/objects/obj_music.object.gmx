    audio_stop_sound(track)
    }
//Now we start bloodmoon theme
if !audio_is_playing(track) &amp;&amp; !audio_is_playing(snd_bloodmoon) 
    {
    
    audio_play_sound(snd_bloodmoon, 10, 0);
    audio_sound_gain(snd_bloodmoon, 0.5, 0)
    }

//Checks if no music is playing
if !audio_is_playing(track) &amp;&amp; !audio_is_playing(snd_bloodmoon){
    music = 0
}
</string>
